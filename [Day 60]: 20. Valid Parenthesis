class Solution:
    def isValid(self, s: str) -> bool:
        dic={')':'(','}':'{',']':'['}
        stack=[]
        for i in s:
            if i=='('or i=='{'or i=='[':
                stack.append(i)
            else:
                if stack==[]:
                    return False
                else:
                    if dic[i]==stack[-1]:
                        stack.pop()
                    else:
                        return False
        return stack==[]

        
