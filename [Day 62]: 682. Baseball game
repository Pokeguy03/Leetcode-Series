class Solution:
    def calPoints(self, operations: List[str]) -> int:
        stack=[]
        summ=0
        for i in operations:
            if i not in {"C","D","+"}:
                stack.append(int(i))
            elif i=="C":
                stack.pop()
            elif i=="D":
                double=stack[-1]*2
                stack.append(double)
            elif i=="+":
                stack.append(stack[-1]+stack[-2])

        return sum(stack)

        
