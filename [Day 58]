class Solution:
    def uncommonFromSentences(self, s1: str, s2: str) -> List[str]:
        s1=s1.split()
        s2=s2.split()
        concate=s1+s2
        li=[]
        count=Counter(concate)
        for i in count:
            if count[i]==1:
                li.append(i)
        return li
        
