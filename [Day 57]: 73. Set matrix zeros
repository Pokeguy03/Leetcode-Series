class Solution:
    def setZeroes(self, matrix: List[List[int]]) -> None:
        row=len(matrix)
        col=len(matrix[0])
        row_s=set()
        col_s=set()
        for i in range(row):
            for j in range(col):
                if matrix[i][j]==0:
                    row_s.add(i)
                    col_s.add(j)
        
        for i in row_s:
            for j in range(col):
                matrix[i][j]=0
        for i in col_s:
            for j in range(row):
                matrix[j][i]=0
        return matrix
        
