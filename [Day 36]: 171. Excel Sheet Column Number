class Solution:
    def titleToNumber(self, columnTitle: str) -> int:
        res=0
        for i in columnTitle:
            res=res*26+(ord(i)-ord('A'))+1  
        return res
    

# AB
# for A:
# ord(A)=65
# ord(i)=65
# ord(i)-ord(A)=0+1
# res=0+1=1=A

# for B:
# res=1(A)
# ord(i)=66
# ord(i)-ord(A)=66-65=1
# res=(1*26)+1=2

# res = 1 * 26 + 2 = 26 + 2 = 28
