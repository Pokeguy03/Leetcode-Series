class Solution:
    def findErrorNums(self, nums: List[int]) -> List[int]:
        n=len(nums)
        count=Counter(nums)
        
        for i in range(1,n+1):
            if count[i]==2:
                dupl=i
            if i not in nums:
                miss=i
        return [dupl,miss]



