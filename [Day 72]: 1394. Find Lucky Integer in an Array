class Solution:
    def findLucky(self, arr: List[int]) -> int:
        res=[]
        count=Counter(arr)
        for i in arr:
            if count[i]==i:
                res.append(i)

        if len(res)==1:
            return res[0]
        elif len(res)>1:
            return max(res)
        else:
            return -1
        
