class Solution:
    def containsNearbyDuplicate(self, nums: List[int], k: int) -> bool:
        l=0
        li=set()
        for r in range(len(nums)):
            if r-l>k:
                li.remove(nums[l])
                l+=1
            if nums[r] in li:
                return True
            li.add(nums[r])
        return False


     
        
