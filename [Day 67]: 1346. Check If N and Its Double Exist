class Solution:
    def checkIfExist(self, arr: List[int]) -> bool:
        for i in range(len(arr)):
            for j in range(len(arr)):
                if i!=j and arr[i]==2*arr[j]:
                    return True
                
        return False


class Solution:
    def checkIfExist(self, arr: List[int]) -> bool:
        s=set()
        for i in arr:
            if((i%2==0 and i//2 in s) or (2*i in s)):
                return True
            else:
                s.add(i)
        return False

        
